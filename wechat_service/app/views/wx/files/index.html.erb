<h1>Listing wx_files</h1>

<table>
  <thead>
    <tr>
      <th>File name</th>
      <th>File size</th>
      <th>Mime type</th>
      <th>Digest</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @wx_files.each do |wx_file| %>
      <tr>
        <td><%= wx_file.file_name %></td>
        <td><%= wx_file.file_size %></td>
        <td><%= wx_file.mime_type %></td>
        <td><%= wx_file.digest %></td>
        <td><%= wx_file.description %></td>
        <td><%= link_to 'Show', wx_file %></td>
        <td><%= link_to 'Edit', edit_wx_file_path(wx_file) %></td>
        <td><%= link_to 'Destroy', wx_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File', new_wx_file_path %>
