<h1>Listing key_words</h1>

<table>
  <thead>
    <tr>
      <th>Key</th>
      <th>Content</th>
      <th>App</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @key_words.each do |key_word| %>
      <tr>
        <td><%= key_word.key %></td>
        <td><%= key_word.content %></td>
        <td><%= key_word.app %></td>
        <td><a href="/key_words/<%= key_word.id %>/edit?app_id=<%= params[:app_id] %>">Edit</a></td>
        <td><a data-confirm="Are you sure?" data-method="delete" href="/key_words/<%= key_word.id %>/?app_id=<%= params[:app_id] %>" rel="nofollow">Destroy</a></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<a href="/key_words/new/?app_id=<%= params[:app_id] %>">New key word</a>

