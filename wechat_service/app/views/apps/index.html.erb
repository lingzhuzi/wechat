<h1>Listing apps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Icon</th>
      <th>Wx</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @apps.each do |wx_app| %>
      <tr>
        <td><%= wx_app.name %></td>
        <td><%= wx_app.icon %></td>
        <td><%= wx_app.wx_id %></td>
        <td><%= link_to 'Show', wx_app %></td>
        <td><%= link_to 'Edit', edit_app_path(wx_app) %></td>
        <td><%= link_to 'Destroy', wx_app, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App', new_app_path %>
